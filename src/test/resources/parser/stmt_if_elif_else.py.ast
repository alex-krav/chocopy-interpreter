statements:
  - statement:
      class: Stmt.If
      condition:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: 1

        operator: ">"
        right:
          class: Expr.Literal
          value: 2


      thenBranch:
        - statement:
            class: Stmt.Print
            expr:
              class: Expr.Literal
              value: 1



      elseBranch:
        - statement:
            class: Stmt.If
            condition:
              class: Expr.Binary
              left:
                class: Expr.Literal
                value: 3

              operator: "=="
              right:
                class: Expr.Literal
                value: 4


            thenBranch:
              - statement:
                  class: Stmt.Print
                  expr:
                    class: Expr.Literal
                    value: 2



            elseBranch:
              - statement:
                  class: Stmt.If
                  condition:
                    class: Expr.Literal
                    value: true

                  thenBranch:
                    - statement:
                        class: Stmt.Print
                        expr:
                          class: Expr.Literal
                          value: 3



                  elseBranch:
                    - statement:
                        class: Stmt.Print
                        expr:
                          class: Expr.Literal
                          value: 4






