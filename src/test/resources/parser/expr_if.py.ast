statements:
  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Ternary
        onTrue:
          class: Expr.Literal
          type: java.lang.Integer
          value: 3

        condition:
          class: Expr.Binary
          left:
            class: Expr.Literal
            type: java.lang.Integer
            value: 1

          operator: ">"
          right:
            class: Expr.Literal
            type: java.lang.Integer
            value: 2


        onFalse:
          class: Expr.Literal
          type: java.lang.Integer
          value: 4



