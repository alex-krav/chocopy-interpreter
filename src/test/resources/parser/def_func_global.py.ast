statements:
  - statement:
      class: Stmt.Var
      name: z
      type: int
      initializer:
        class: Expr.Literal
        value: 0


  - statement:
      class: Stmt.Function
      name: foo
      params:
        - param:
            class: Stmt.Var
            name: x
            type: int
            initializer: null

      returnType: bool
      body:
        - statement:
            class: Stmt.Global
            name: z

        - statement:
            class: Stmt.Return
            value:
              class: Expr.Binary
              left:
                class: Expr.Variable
                name: x

              operator: ">"
              right:
                class: Expr.Variable
                name: z




  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Call
        callee:
          class: Expr.Variable
          name: foo

        arguments:
          - argument:
              class: Expr.Literal
              value: 1



