statements:
  - statement:
      class: Stmt.Function
      name: foo
      params:
        - param:
            class: Stmt.Var
            name: x
            type: int
            initializer: null

      returnType: bool
      body:
        - statement:
            class: Stmt.Var
            name: a
            type: int
            initializer:
              class: Expr.Literal
              type: java.lang.Integer
              value: 0


        - statement:
            class: Stmt.Var
            name: b
            type: int
            initializer:
              class: Expr.Literal
              type: java.lang.Integer
              value: 1


        - statement:
            class: Stmt.Function
            name: bar
            params:
              - param:
                  class: Stmt.Var
                  name: y
                  type: int
                  initializer: null

            returnType: int
            body:
              - statement:
                  class: Stmt.Nonlocal
                  name: a

              - statement:
                  class: Stmt.Expression
                  expr:
                    class: Expr.Assign
                    name: a
                    value:
                      class: Expr.Literal
                      type: java.lang.Integer
                      value: 2



              - statement:
                  class: Stmt.Return
                  value:
                    class: Expr.Variable
                    name: y



        - statement:
            class: Stmt.Return
            value:
              class: Expr.Binary
              left:
                class: Expr.Call
                callee:
                  class: Expr.Variable
                  name: bar

                arguments:
                  - argument:
                      class: Expr.Variable
                      name: x


              operator: ">"
              right:
                class: Expr.Variable
                name: a




  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Call
        callee:
          class: Expr.Variable
          name: foo

        arguments:
          - argument:
              class: Expr.Literal
              type: java.lang.Integer
              value: 1



