              - statement:
                  class: Stmt.Expression
                  expr:
                    class: Expr.Print
                    expr:
                      class: Expr.Literal
                      inferredType: str
                      value: "B"
  - statement:
      class: Stmt.Var
      name: a
      type: A
      initializer:
        class: Expr.Literal
        inferredType: <None>
        value: None
