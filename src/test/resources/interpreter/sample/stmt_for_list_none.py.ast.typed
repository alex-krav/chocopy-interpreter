statements:
  - statement:
      class: Stmt.Var
      name: x
      type: int
      initializer:
        class: Expr.Literal
        inferredType: int
        value: 0
  - statement:
      class: Stmt.Var
      name: z
      type: [int]
      initializer:
        class: Expr.Literal
        inferredType: <None>
        value: None
  - statement:
      class: Stmt.For
      id:
        class: Expr.Variable
        inferredType: int
        name: x
      iterable:
        class: Expr.Variable
        inferredType: [int]
        name: z
      body:
        - statement:
            class: Stmt.Expression
            expr:
              class: Expr.Print
              expr:
                class: Expr.Variable
                inferredType: int
                name: x
