statements:
  - statement:
      class: Stmt.Var
      name: x
      type: str
      initializer:
        class: Expr.Literal
        inferredType: str
        value: "xXx"
  - statement:
      class: Stmt.For
      id:
        class: Expr.Variable
        inferredType: str
        name: x
      iterable:
        class: Expr.Variable
        inferredType: str
        name: x
      body:
        - statement:
            class: Stmt.Expression
            expr:
              class: Expr.Print
              expr:
                class: Expr.Variable
                inferredType: str
                name: x
