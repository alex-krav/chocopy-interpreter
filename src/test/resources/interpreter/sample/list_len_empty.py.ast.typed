statements:
  - statement:
      class: Stmt.Var
      name: x
      type: [int]
      initializer:
        class: Expr.Literal
        inferredType: <None>
        value: None
  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.MultiAssign
        targets:
          - target:
              class: Expr.Variable
              inferredType: [int]
              name: x
        value:
          class: Expr.Listing
          inferredType: <Empty>
          elements: []
  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Print
        expr:
          class: Expr.Len
          expr:
            class: Expr.Variable
            inferredType: [int]
            name: x
