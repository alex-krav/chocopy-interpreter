      class: Stmt.Var
      name: x
      type: int
      initializer:
        class: Expr.Literal
        inferredType: int
        value: 1
  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        inferredType: int
        left:
          class: Expr.Variable
          inferredType: int
