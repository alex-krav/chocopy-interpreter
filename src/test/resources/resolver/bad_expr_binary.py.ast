statements:
  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Logical
        left:
          class: Expr.Literal
          value: 1

        operator: "and"
        right:
          class: Expr.Literal
          value: 2



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Logical
        left:
          class: Expr.Literal
          value: 1

        operator: "or"
        right:
          class: Expr.Literal
          value: 2



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: true

        operator: "-"
        right:
          class: Expr.Literal
          value: false



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: true

        operator: "<"
        right:
          class: Expr.Literal
          value: false



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: 1

        operator: "+"
        right:
          class: Expr.Literal
          value: true



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: false

        operator: "+"
        right:
          class: Expr.Literal
          value: 0



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: 1

        operator: "=="
        right:
          class: Expr.Literal
          value: true



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: false

        operator: "!="
        right:
          class: Expr.Literal
          value: 0



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: 1

        operator: "is"
        right:
          class: Expr.Literal
          value: 1



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: true

        operator: "is"
        right:
          class: Expr.Literal
          value: false



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: None

        operator: "+"
        right:
          class: Expr.Literal
          value: None



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: None

        operator: "=="
        right:
          class: Expr.Literal
          value: None



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Literal
          value: None

        operator: "is"
        right:
          class: Expr.Literal
          value: None



