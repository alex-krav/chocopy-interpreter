statements:
  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Ternary
        inferredType: int
        onTrue:
          class: Expr.Literal
          inferredType: int
          value: 3

        condition:
          class: Expr.Binary
          inferredType: bool
          left:
            class: Expr.Literal
            inferredType: int
            value: 1

          operator: ">"
          right:
            class: Expr.Literal
            inferredType: int
            value: 2


        onFalse:
          class: Expr.Literal
          inferredType: int
          value: 4



