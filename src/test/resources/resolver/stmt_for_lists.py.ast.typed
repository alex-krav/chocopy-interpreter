statements:
  - statement:
      class: Stmt.Var
      name: x
      type: int
      initializer:
        class: Expr.Literal
        inferredType: int
        value: 0


  - statement:
      class: Stmt.For
      identifier:
        class: Expr.Variable
        inferredType: int
        name: x

      iterable:
        class: Expr.Listing
        inferredType: [int]
        elements:
          - element:
              class: Expr.Literal
              inferredType: int
              value: 1

          - element:
              class: Expr.Literal
              inferredType: int
              value: 2

          - element:
              class: Expr.Literal
              inferredType: int
              value: 3


      body:
        - statement:
            class: Stmt.Expression
            expr:
              class: Expr.Binary
              inferredType: int
              left:
                class: Expr.Variable
                inferredType: int
                name: x

              operator: "+"
              right:
                class: Expr.Literal
                inferredType: int
                value: 1





