statements:
  - statement:
      class: Stmt.Var
      name: x
      type: int
      initializer:
        class: Expr.Literal
        value: 1


  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Binary
        left:
          class: Expr.Variable
          name: x

        operator: "-"
        right:
          class: Expr.Literal
          value: 1



