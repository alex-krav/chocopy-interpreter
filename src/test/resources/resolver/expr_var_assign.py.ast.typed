statements:
  - statement:
      class: Stmt.Var
      name: x
      type: int
      initializer:
        class: Expr.Literal
        inferredType: int
        value: 1
  - statement:
      class: Stmt.Var
      name: o
      type: object
      initializer:
        class: Expr.Literal
        inferredType: <None>
        value: None
  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.MultiAssign
        targets:
          - target:
              class: Expr.Variable
              inferredType: int
              name: x
          - target:
              class: Expr.Variable
              inferredType: object
              name: o
        value:
          class: Expr.Literal
          inferredType: int
          value: 42
