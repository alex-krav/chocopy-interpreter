statements:
  - statement:
      class: Stmt.Var
      name: x
      type: int
      initializer:
        class: Expr.Literal
        value: 0


  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Assign
        target:
          class: Expr.Variable
          name: x

        value:
          class: Expr.Ternary
          onTrue:
            class: Expr.Literal
            value: "Hello"

          condition:
            class: Expr.Binary
            left:
              class: Expr.Literal
              value: 2

            operator: ">"
            right:
              class: Expr.Literal
              value: 3


          onFalse:
            class: Expr.Literal
            value: 3




