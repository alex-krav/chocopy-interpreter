statements:
  - statement:
      class: Stmt.Var
      name: x
      type: [int]
      initializer:
        class: Expr.Literal
        value: None


  - statement:
      class: Stmt.Var
      name: y
      type: [object]
      initializer:
        class: Expr.Literal
        value: None


  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Assign
        target:
          class: Expr.Variable
          name: x

        value:
          class: Expr.Listing
          elements:
            - element:
                class: Expr.Literal
                value: 1

            - element:
                class: Expr.Literal
                value: 2




  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.Assign
        target:
          class: Expr.Variable
          name: y

        value:
          class: Expr.Listing
          elements:
            - element:
                class: Expr.Literal
                value: None




  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.ListSet
        list:
          class: Expr.Variable
          name: x

        index:
          class: Expr.Literal
          value: 0

        value:
          class: Expr.Literal
          value: 3



  - statement:
      class: Stmt.Expression
      expr:
        class: Expr.ListSet
        list:
          class: Expr.Variable
          name: x

        index:
          class: Expr.Literal
          value: 1

        value:
          class: Expr.ListSet
          list:
            class: Expr.Variable
            name: y

          index:
            class: Expr.Literal
            value: 0

          value:
            class: Expr.Literal
            value: 4




